    "c = [0]*100\n",
    "x = [10, 20, 30]\n",
    "avg = sum(x)/len(x)\n",
    "pos, neg = 0, 0\n",
    "for i in range(len(x)):\n",
    "    v = (x[i]-avg)*1.0\n",
    "    if v > 0:\n",
    "        pos += v\n",
    "    else:\n",
    "        neg += v\n",
    "neg = -neg\n",
    "print(neg, pos)"
